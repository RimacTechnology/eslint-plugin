{
  "root": true,
  "extends": [
    "eslint:recommended",
    "plugin:eslint-plugin/all",
    "plugin:node/recommended"
  ],
  "plugins": ["eslint-plugin"],
  "parserOptions": {
    "ecmaVersion": 2017
  },
  "env": {
    "es6": true,
    "node": true
  },
  "rules": {
    "eslint-plugin/require-meta-docs-url": "off",
    "eslint-plugin/require-meta-docs-description": "off"
  }
}
