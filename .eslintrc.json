{
    "root": true,
    "extends": [
        "@rimac-technology/eslint-config/core",
        "plugin:eslint-plugin/all"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "project": "./tsconfig.json"
    },
    "plugins": [
        "eslint-plugin"
    ],
    "ignorePatterns": [
        "*.json",
        "file.ts",
        "file.tsx"
    ],
    "rules": {
        "eslint-plugin/require-meta-docs-url": "off",
        "eslint-plugin/require-meta-docs-description": "off"
    }
}
